{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "HR-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/hr-frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/animate.css/animate.min.css",
              "src/styles.scss",
              "src/assets/css/plugins/apexcharts.css",
              "src/assets/css/plugins/buttons.bootstrap5.css",
              "src/assets/css/plugins/datatables.min.css",
              "src/assets/css/plugins/dropify.min.css",
              "src/assets/css/plugins/flatpickr.min.css",
              "src/assets/css/plugins/fullcalendar.min.css",
              "src/assets/css/plugins/ion.rangeSlider.min.css",
              "src/assets/css/plugins/jquery-jvectormap-2.0.5.css",
              "src/assets/css/plugins/jquery-ui.css",
              "src/assets/css/plugins/jquery.dataTables.min.css",
              "src/assets/css/plugins/jquery.timepicker.css",
              "src/assets/css/plugins/nano.min.css",
              "src/assets/css/plugins/select2.min.css",
              "src/assets/css/plugins/simplebar.css",
              "src/assets/css/plugins/swiper-bundle.min.css",
              "src/assets/css/plugins/tagify.css",
              "src/assets/css/plugins/waves.min.css",
              "src/assets/css/vendor/animate.css",
              "src/assets/css/vendor/bootstrap.min.css",
              "src/assets/css/vendor/dropzone.css",
              "src/assets/css/vendor/fontawesome-pro.css",
              "src/assets/css/vendor/icomoon.css",
              "src/assets/css/vendor/magnific-popup.css",
              "src/assets/css/vendor/rating.css",
              "src/assets/css/vendor/spacing.css",
              "src/assets/css/main.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "src/assets/js/plugins/apexcharts.min.js",
              "src/assets/js/plugins/backtotop.js",
              "src/assets/js/plugins/bloodhound.js",
              "src/assets/js/plugins/buttons.colVis.min.js",
              "src/assets/js/plugins/buttons.html5.min.js",
              "src/assets/js/plugins/buttons.print.min.js",
              "src/assets/js/plugins/cleave.min.js",
              "src/assets/js/plugins/custom.js",
              "src/assets/js/plugins/dataTables.bootstrap5.min.js",
              "src/assets/js/plugins/dataTables.buttons.min.js",
              "src/assets/js/plugins/datatables.min.js",
              "src/assets/js/plugins/dayjs.min.js",
              "src/assets/js/plugins/dropify.min.js",
              "src/assets/js/plugins/dropzone.js",
              "src/assets/js/plugins/flatpickr.js",
              "src/assets/js/plugins/fullcalendar.min.js",
              "src/assets/js/plugins/ion.rangeSlider.min.js",
              "src/assets/js/plugins/jquery-ui.min.js",
              "src/assets/js/plugins/jquery.dataTables.min.js",
              "src/assets/js/plugins/jquery.timepicker.min.js",
              "src/assets/js/plugins/jsvectormap.min.js",
              "src/assets/js/plugins/jszip.min.js",
              "src/assets/js/plugins/loader.js",
              "src/assets/js/plugins/pdfmake.min.js",
              "src/assets/js/plugins/popper.min.js",
              "src/assets/js/plugins/select2.full.min.js",
              "src/assets/js/plugins/simplebar-active.js",
              "src/assets/js/plugins/smooth-scrollbar.js",
              "src/assets/js/plugins/steps-form.js",
              "src/assets/js/plugins/sweetalert2.all.min.js",
              "src/assets/js/plugins/swiper-bundle.min.js",
              "src/assets/js/plugins/tagify.js",
              "src/assets/js/plugins/tinymce.min.js",
              "src/assets/js/plugins/typeahead.bundle.min.js",
              "src/assets/js/plugins/vfs_fonts.js",
              "src/assets/js/plugins/waypoints.min.js",
              "src/assets/js/plugins/world-merc.js",
              "src/assets/js/vendor/ajax-form.js",
              "src/assets/js/vendor/app-invoice-print.js",
              "src/assets/js/vendor/bootstrap.bundle.min.js",
              "src/assets/js/vendor/calendar-activision.js",
              "src/assets/js/vendor/custom-chart.js",
              "src/assets/js/vendor/custom-tagify.js",
              "src/assets/js/vendor/height-equal.js",
              "src/assets/js/vendor/isotope.pkgd.js",
              "src/assets/js/vendor/jquery-3.7.0.js",
              "src/assets/js/vendor/jquery.barrating.js",
              "src/assets/js/vendor/jquery.repeater.js",
              "src/assets/js/vendor/magnific-popup.min.js",
              "src/assets/js/vendor/rangeslider-script.js",
              "src/assets/js/vendor/rating-script.js",
              "src/assets/js/vendor/read-more-btn.js",
              "src/assets/js/vendor/sidebar.js",
              "src/assets/js/main.js"

            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "12MB",
                  "maximumError": "15MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "40kB",
                  "maximumError": "50kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "HR-frontend:build:production"
            },
            "development": {
              "buildTarget": "HR-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
